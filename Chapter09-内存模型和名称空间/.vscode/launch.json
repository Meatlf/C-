  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) external",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/external",
      //"processId": "${command:pickProcess}",
      "cwd": "${workspaceRoot}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
